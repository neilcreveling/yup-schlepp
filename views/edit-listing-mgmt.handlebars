


{{> nav}}
<div class="mgmt-id hide">{{mgmt.id}}</div>
<div class="container main-card mt-5">
    <div class="row d-flex justify-content-center mgmt-card">
        <div class="card col-md address-input m-2 p-0 inner-card">
            <div class="card-body container p-0">
                <div class="row d-flex justify-content-center">
                    <div class="col p-3">
                        <h2>Rename Management Company: "{{mgmt.management_name}}"</h2>
                    </div>
                </div>
                <hr>
                <form class='mgmt-form'>
                  <div class='row d-flex justify-content-center add-row'>
                    <hr />
                    <div class='form-group col-12 p-4'>
                      <label for='management-company'>Updated Name:</label>
                      <input type='text' class='form-control' id='mgmt' placeholder='' />
                    </div>
                  </div>
                  <div class='card-footer d-flex justify-content-end'>
                    <div class='col-3 d-flex'>
                      <button type='button' class='btn btn-outline-dark col-6 m-2 cancel-btn'>Cancel</button>
                      <button type='submit' class='btn btn-outline-dark col-6 m-2 next-btn'>Update</button>
                    </div>
                  </div>
                </form>
            </div>
        </div>
    </div>
</div>
<script src='/js/edit-mgmt.js'></script>
