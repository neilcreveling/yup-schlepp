{{> nav}}
<h2 class="property-form-title">Submit Changes to [Address of Listing]</h2>

<form class="property-form">
    <div>
        <input type="text" id="edit-address" class="address-field" name="Address" placeholder="address"/>
    </div>
    <div>
        <textarea id="edit-description" class="property-description" name="description" rows="5" cols="33" placeholder="Description"></textarea>
    </div>
    <div>
        <button type="submit" id="edit-listing-submit" class="submit-property">Create</button>
    </div>
</form>